{
  "extends": ["airbnb", "prettier"],
  "plugins": ["prettier", "testing-library"],
  "rules": {
    "prettier/prettier": "warn",
    "react/jsx-filename-extension": "off",
    "react/jsx-one-expression-per-line": [1, "literal"]
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module"
  },
  "env": {
    "jest": true
  },
  "ignorePatterns": ["dist/*"]
}
