{
  "name": "bearbnb-frontend",
  "version": "1.0.0",
  "description": "the react frontend for a bear-themed airbnb clone",
  "main": "index.js",
  "scripts": {
    "dev": "parcel src/index.html",
    "test": "jest --watch",
    "snapshot": "jest storybook",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jmtes/bearbnb-frontend.git"
  },
  "author": "Juno Tesoro",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/jmtes/bearbnb-frontend/issues"
  },
  "homepage": "https://github.com/jmtes/bearbnb-frontend#readme",
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.11.0",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.1",
    "@babel/register": "^7.11.5",
    "@storybook/addon-a11y": "^6.0.26",
    "@storybook/addon-actions": "^6.0.26",
    "@storybook/addon-essentials": "^6.0.26",
    "@storybook/addon-links": "^6.0.26",
    "@storybook/addon-storyshots": "^6.0.26",
    "@storybook/react": "^6.0.26",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "core-js": "^3.6.5",
    "env-cmd": "^10.1.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "eslint": "^7.10.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.0.2",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.2",
    "eslint-plugin-react-hooks": "^4.1.2",
    "jest": "^26.4.2",
    "parcel-bundler": "^1.12.4",
    "prettier": "^2.1.2",
    "react-is": "^16.13.1"
  },
  "dependencies": {
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-router-dom": "^5.2.0",
    "styled-components": "^5.2.0",
    "styled-normalize": "^8.0.7"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "./tests/config/setupTests.js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|svg)$": "<rootDir>/tests/__mocks__/fileMock.js",
      "\\.css$": "<rootDir>/tests/__mocks__/styleMock.js"
    }
  }
}
